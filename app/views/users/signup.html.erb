<%= form_for(@user) do |f| %>
		<p>Email: <%= f.text_field :email%></p>
		<p>Password: <%= f.password_field :password %></p>
		<p>Password Confirmation: <%= f.password_field :password_confirmation %> </p>
		<div class="top-login-input-float-submit"><button type="submit" class="blueformbutton">Join</button></div>
		<div class="clear"></div>
<% end %>